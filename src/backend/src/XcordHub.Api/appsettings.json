{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    }
  },
  "Database": {
    "ConnectionString": ""
  },
  "Redis": {
    "ConnectionString": "localhost:6379",
    "ChannelPrefix": "hub"
  },
  "Jwt": {
    "Issuer": "xcord-hub",
    "Audience": "xcord-hub-clients"
  },
  "Cors": {
    "AllowedOrigins": ["http://localhost:3000"]
  },
  "RateLimiting": {
    "TokenLimit": 100,
    "ReplenishmentPeriodSeconds": 10,
    "TokensPerPeriod": 20
  },
  "Docker": {
    "SocketProxyUrl": "http://docker-socket-proxy:2375",
    "UseReal": false
  },
  "Caddy": {
    "AdminUrl": "http://caddy:2019",
    "UseReal": false
  },
  "Cloudflare": {
    "ApiToken": "",
    "ZoneId": "",
    "DomainName": "",
    "UseReal": false
  },
  "Hub": {
    "BaseDomain": "xcord-dev.net"
  },
  "Email": {
    "SmtpHost": "mailpit",
    "SmtpPort": 1025,
    "SmtpUsername": "",
    "SmtpPassword": "",
    "FromAddress": "noreply@xcord.local",
    "FromName": "Xcord",
    "UseSsl": false,
    "DevMode": true,
    "HubBaseUrl": "http://localhost:3000"
  },
  "Auth": {
    "BcryptWorkFactor": 12
  }
}
